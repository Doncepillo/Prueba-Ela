@model Profesores  <div class="row">     <h1> Mantenedor Profesores </h1>     @using (Html.BeginForm("Ficha", "Home", FormMethod.Post, new { @class = "form-horizontal" }))    {         <div class="form-group">             <label class="control-label col-sm-2" for="rut">Rut</label>              <div class="col-sm-6">                 @Html.TextBoxFor(m => m.rut, null, new { @class = "form-control" })             </div>         </div>         <div class="form-group">             <label class="control-label col-sm-2" for="nombrecompleto">Nombre Completo</label>             <div class="col-sm-6">                 @Html.TextBoxFor(m => m.nombrecompleto, null, new { @class = "form-control" })             </div>         </div>          <div class="form-group">             <label class="control-label col-sm-2" for="titulo">Título</label>             <div class="col-sm-6">                 @Html.TextBoxFor(m => m.titulo, null, new { @class = "form-control" })             </div>         </div>                        <div class="form-group">             <label class="control-label col-sm-2" for="grado">Grado Profesional</label>             <div class="col-sm-6">                 @Html.DropDownListFor(m=>m.grado, new List<SelectListItem>                                         {                         new SelectListItem{Text="Seleccione...", Value=""},                         new SelectListItem{Text="Técnico Superior", Value="Técnico Superior"},                         new SelectListItem{Text="Ingeniero", Value="Ingeniero"},                         new SelectListItem{Text="Magister", Value="Magister"},                         new SelectListItem{Text="Doctor", Value="Doctor"}                     },                      new { @class="form-control"})                                </div>         </div>          <div class="form-group">             <div class="col-sm-offset-2 col-sm-1">                 <button type="reset" class="btn btn-info">Limpiar</button>             </div>             <div class="col-sm-offset-1 col-sm-1">                 <button type="submit" class="btn btn-primary">Guardar</button>             </div>         </div>     } </div> 